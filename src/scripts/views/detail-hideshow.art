<header class="navbar">
    <div class="back">
        <</div> <h1 class="nav-header">{{detailitem.nm}}</h1>
</header>

<div class="total-body">
    <div class="download-header">
        <div class="logo-img"></div>
        <div class="info-wrapper">
            <div class="title">猫眼</div>
            <div class="content">0 元看电影，天天领现金</div>
        </div>
        <div class="link-btn">立即领取
            <div class="wordafter"></div>
        </div>
    </div>
</div>

<div class="movie-detail">
    <div class="movie-filter"></div> <!-- 背景颜色 -->
    <div class="poster-bg"></div> <!-- 背景图片 -->

    <div class="detailflex-box">
        <div class="poster">
            <img src="{{detailitem.img.replace(/w.h/,'128.180')}}" alt="">
        </div>

        <div class="contentflex">
            <div class="title-one line-ellipsis">{{detailitem.nm}}</div>
            <div class="title-two line-ellipsis">{{detailitem.enm}}</div>

            <div class="score">
                {{detailitem.sc}}

                <span class="snum line-ellipsis">({{(detailitem.snum/10000).toFixed(1)}}万人评)</span>
            </div>
            <div class="type">
                <span>{{detailitem.cat}}</span>

                <div class="type-group">
                    <img src="data:image/png;base64,"
                        alt="">
                </div>

            </div>
            <div class="src line-ellipsis">{{detailitem.src}}/{{detailitem.dur}}分钟</div>
            <div class="pubdesc line-ellipsis">{{detailitem.pubDesc}}</div>
        </div>

    </div>

    <div class="arrow-g">
        <img
            src="data:image/png;base64,">
    </div>

</div>